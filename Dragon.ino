#include <Servo.h>

Servo s1;  // create dragon shoulder joint wing thingy
int pos = 0;    // variable to store the joint position

void setup() {
  s1.attach(9);  // attaches the servo on pin 9 to the servo object
}

void loop() {
  for (pos = -90; pos <= 90; pos += 1) { // goes from 0 degrees to 180 degrees
    // in steps of 1 degree
    s1.write(pos);
    delay(15);                       // waits 15ms for the servo to reach the position
  }
  for (pos = 90; pos >= -90; pos -= 1) { // goes from 180 degrees to 0 degrees
    s1.write(pos);
    delay(15);                       // waits 15ms for the servo to reach the position
  }
}
